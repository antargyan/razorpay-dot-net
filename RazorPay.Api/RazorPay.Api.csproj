<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
    <PackageId>RazorPay.Api</PackageId>
    <Version>1.0.0</Version>
    <Authors>Sameer Dube</Authors>
    <Description>RazorPay API client library for .NET Standard 2.1</Description>
    <PackageTags>razorpay;api;payments;client</PackageTags>
    <RepositoryUrl>https://github.com/antargyan/razorpay-dot-net</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BouncyCastle.Cryptography" Version="2.6.2" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>
